    <dependencies>
      <dependency id="Common.Logging" version="[3.0.0,)" />
    </dependencies>
  </metadata>
  <files>
    <file src="lib\JamaaTech.Smpp.Net*.*" target="lib" exclude="**\*.exe" />    
  </files>
</package>