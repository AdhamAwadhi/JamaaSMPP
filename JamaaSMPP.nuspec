<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>JamaaSMPP</id>
    <version>1.7.0</version>
    <title>Jamaa SMPP Client</title>
    <authors>JamaaTech</authors>
